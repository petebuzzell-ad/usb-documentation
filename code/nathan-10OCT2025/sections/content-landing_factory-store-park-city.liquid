{% for block in section.blocks %}
  {% case block.type %}

    {% when 'hero_image' %}
      {% render 'snippet-hero-image' block: block, number: forloop.index %}

    {% when '2_grid_cta' %}
      {% render 'snippet-2-grid-cta' block: block, number: forloop.index %}

    {% when 'full_width_cta_reversible' %}
      {% render 'snippet-full-width-cta-reversible' block: block, number: forloop.index %}

    {% when 'text_block' %}
      {% render 'snippet-text-block' block: block, number: forloop.index %}

    {% when 'full_width_cta' %}
      {% render 'snippet-full-width-cta' block: block, number: forloop.index %}

    {% when 'centered_content_block' %}
      {% render 'snippet-centered-content' block: block, number: forloop.index %}
    
    {% when 'accordion' %}
      {% include 'snippet-accordion2', block: block, underline: true, number: forloop.index %}
    
    {% when 'image_content_slider' %}
      {% include 'snippet-image-content-slider' block: block, number: forloop.index %}
    
    {% when 'image_gallery_slider' %}
      {% include 'snippet-image-gallery-slider', block: block, underline: true, number: forloop.index %}
    
    {% when '3_grid_cta' %}
      {% render 'snippet-3-grid-cta', block: block, underline: true, number: forloop.index %}

    {% when 'suggested_products' %}
      {% render 'snippet-suggested-products', block: block, underline: true, number: forloop.index %}
          
    {% when 'spacer' %}
      {% render 'snippet-spacer' block: block, number: forloop.index %}
      
    {% when 'shop_by_collection' %}
      {% render 'snippet-shop-by-collection' block: block, number: forloop.index %}    

    {% when 'category_filter' %}
      {% render 'snippet-category-filter' block: block, number: forloop.index %}          

  {% endcase %}
{% endfor %}

