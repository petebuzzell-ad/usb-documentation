                  <img class="lazy w-11/12  h-11/12 object-contain" data-src="{{product.featured_image | img_url: '92x', scale: 2 }}" alt="{{product.featured_image.alt}}">
                </div>
              {%- endif -%}
              <div>
                <div class="text-p-s mb-0.5 sm:hover:underline sm:hover:text-pine">{{- product.title -}}</div>
                <div class="font-sora text-p-s sm:text-p text-stone leading-5 font-semibold">{{- product.price | money -}}</div>
              </div>
            </a>
          </li>
          <li>
            {%- assign product = collections.all.products.last -%}
            <a href="{{product.url}}" class="flow-root">
              {%- if product.featured_image != blank -%}
                <div class="inline-flex items-center justify-center w-26.5 h-26.5 mr-6.25 bg-faint-cloud float-left">
                  <img class="lazy w-11/12 h-11/12 object-contain" data-src="{{product.featured_image | img_url: '92x', scale: 2 }}" alt="{{product.featured_image.alt}}">
                </div>
              {%- endif -%}
              <div>
                <div class="text-p-s mb-0.5 sm:hover:underline sm:hover:text-pine">{{- product.title -}}</div>
                <div class="font-sora text-p-s sm:text-p text-stone leading-5 font-semibold">{{- product.price | money -}}</div>
              </div>
            </a>
          </li>
        </ul>
      </div>
      <div class="search-auto__suggest relative bg-light-sand w-full sm:w-1/2 sm:w-270 p-5 sm:p-8 xl:flex justify-between  flex-col">
        <span class="hidden sm:block w-4 h-4 shadow-search-auto bg-light-sand transform rotate-45 z-1 absolute left-1/2 -translate-x-1/2 -top-2.5">
          <span class="w-8 h-8 bg-light-sand absolute z-2 top-0 left-0"></span>
        </span>
        <ul class="list-none p-0">
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine">
            <b>Mens Jersey</b>
          </li>
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine">
            <a>Short Sleeve <b>Mens Jersey</b></a>
          </li>
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine"><b>Mens</b> Top</li>
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine"><b>Mens</b> Shirts</li>
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine">Womens <b>Jersey</b></li>
          <li class="search-auto__suggest--item text-p-s leading-9 sm:hover:underline sm:hover:text-pine">Tops</li>
        </ul>
        <a href="javascript:void(0)" class="search-auto--all w-max uppercase font-sora text-special-xs sm:text-special border border-t-0 border-l-0 border-r-0 border-b-2 border-black font-semibold sm:hover:border-pine sm:hover:text-pine mt-2 md:mt-0">All Results</a>
      </div>
    </div>
  {% endif %}

