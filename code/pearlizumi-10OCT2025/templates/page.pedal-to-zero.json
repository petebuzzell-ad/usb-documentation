    "full_width_cta_K9PinY": {
      "type": "full-width-cta",
      "name": "Full Width CTA",
      "settings": {
        "image": "shopify://shop_images/banner_986cc4db-61ca-4389-b862-eae039d5d014.jpg",
        "image_mobile": "shopify://shop_images/bannermob_f6afe009-bb5b-4d85-b4b0-80a2c59c1fbf.jpg",
