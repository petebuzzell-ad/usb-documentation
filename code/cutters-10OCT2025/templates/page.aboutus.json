    "section_image_with_text_N69Ft6": {
      "type": "section-image-with-text",
      "blocks": {
        "heading_RqAHjd": {
          "type": "heading",
          "settings": {
            "title": "<p>REV-TACK <em>RECHARGE </em>TECHNOLOGY</p>",
