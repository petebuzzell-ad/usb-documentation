    "section_image_with_text_FpGwzf": {
      "type": "section-image-with-text",
      "blocks": {
        "heading_dDqptp": {
          "type": "heading",
          "settings": {
            "title": "<p>C-TACK® <em>PERFORMANCE GRIP</em></p>",
