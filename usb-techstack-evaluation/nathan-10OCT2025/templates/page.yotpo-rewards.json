    "yotpo_faq_Cakayp": {
      "type": "yotpo-faq",
      "blocks": {
        "faq_gXCNyJ": {
          "type": "faq",
          "settings": {
            "question": "How do I redeem my NATHAN Points?",
            "answer": "<p>Points will be available to redeem for rewards during checkout. As a reminder, points cannot be combined with any other discounts or promotions.</p><p><span style=\"text-decoration:underline\"><strong>Note</strong></span><strong>:</strong> If the number of points redeemed is greater than your order total, <span style=\"text-decoration:underline\">you will lose</span> the difference in points (example: if an order is $8 and a $10 reward is redeemed, the remaining $2 will be lost.)</p>"
          }
        },
        "faq_rXhpXV": {
          "type": "faq",
          "settings": {
            "question": "What can I redeem my NATHAN Points on?",
            "answer": "<p>Points may be redeemed for rewards on all products!  Customers can redeem points for rewards at 100 points. Rewards are offered at $10 increments. Maximum reward </p>"
          }
        },
        "faq_mKEXgC": {
          "type": "faq",
          "settings": {
            "question": "How Do Points Convert to dollars?",
            "answer": "<p>100 points = $10 (i.e., 10 points = $1; each point is worth $0.10).<br/>Redemptions are available in $10 increments at checkout (100 pts = $10, 200 pts = $20, 300 pts = $30, etc.).</p><ul><li>Explorer (Tier 1): Earn 1 point per $1. Example: spend $150 → 150 points = $15.</li><li>Trail Blazer (Tier 2): Earn points 2× as fast. Example: spend $150 → 300 points = $30.</li><li>VIP (Tier 3): Earn points 3× as fast. Example: spend $150 → 450 points = $45.</li></ul><p><em>The value of your points doesn’t change by tier—higher tiers just earn points faster.</em></p>"
          }
        }
      },
